{
  "name": "healthcheckgraphqldemo",
  "version": "1.0.0",
  "description": "demo implementing bluemix monitoring with graphql",
  "main": "nodemon ./server.js --exec babel-node",
  "scripts": {
    "graphscript": "nodemon ./server.js --exec babel-node",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint .",
    "reactscript": "react-scripts start",
    "start": "npm run graphscript && npm run reactscript",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/Naresh-Eeda/HealthCheckGraphQLDemo.git"
  },
  "keywords": [
    "graphql",
    "monitoring",
    "dashboard"
  ],
  "author": "Naresh Eeda",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "casual": "^1.5.14",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "express-graphql": "*",
    "graphql": "^0.10.5",
    "graphql-date": "*",
    "graphql-server-express": "*",
    "graphql-server-hapi": "*",
    "graphql-server-lambda": "*",
    "graphql-subscriptions": "*",
    "graphql-tools": "*",
    "hapi": "*",
    "jQuery": "^1.7.4",
    "jquery": "^3.2.1",
    "jsdom": "^11.1.0",
    "json-to-graphql": "*",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "request-promise": "^4.2.1",
    "subscriptions-transport-ws": "*"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "nodemon": "^1.9.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
